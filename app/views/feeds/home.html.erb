<% if signed_in? %>
<div class="col-md-6 col-md-offset-3 content">
  <div class="name">
    <%= @week.date %>
  </div>
  <% if @week.next %>
    <%= link_to @week.next.date, root_path(:week => @week.next.id) %>
  <% end %>
  <% if @week.previous %>
    <%= link_to @week.previous.date, root_path(:week => @week.previous.id) %>
  <% end %>
  <div class="list-group" id="group">
    <%= render 'shared/feed' %>
  </div>
</div>
<% else %>
  <div class="jumbotron">
    <center>
      <h1>The Weekend</h1>
      <%= render 'shared/signin' %>
    </center>
  </div>
<% end %>
