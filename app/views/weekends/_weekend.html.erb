<div class="list-group-item">
  <div class="name">
    <% if @user %>
      <%= weekend.week.date %>
    <% else %>
      <%= link_to weekend.user.name, weekend.user %>
    <% end %>
  </div>
  <div class="vote_count" data-weekend-id="<%= weekend.id %>"><%= (weekend.votes_count) %> votes</div>
  <%= render 'weekends/vote_form', :weekend => weekend %>
  <%= render 'shared/weekend_images', :weekend => weekend %>
</div>
