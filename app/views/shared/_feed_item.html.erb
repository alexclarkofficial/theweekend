<% weekends.each do |weekend| %>
  <div class="list-group-item">
    <div class="name"><center><%= link_to weekend.user.name, weekend.user %></center></div>
    <div class="vote_count" data-weekend-id="<%= weekend.id %>"> <%= weekend.votes.count %> Votes </div>
    <%= render 'weekends/vote_form', :weekend => weekend %>
    <%= render 'shared/weekend_images', :weekend => weekend %>
  </div>
<% end %>